{
  "name": "swiftshop-ecommerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "SwiftShop - A modern e-commerce platform built with React.js, Redux, and Tailwind CSS. Features include user authentication, shopping cart, wishlist, product search, and responsive design.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "deploy": "npm run build && vercel --prod",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "react",
    "redux",
    "ecommerce",
    "tailwind",
    "vite",
    "shopping-cart",
    "responsive",
    "web-development",
    "capstone-project",
    "swiftshop"
  ],
  "author": {
    "name": "Mohit Shiv",
    "email": "mohitshiv995@gmail.com",
    "url": "https://github.com/mohitshivv"
  },
