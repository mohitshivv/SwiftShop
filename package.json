  "repository": {
    "type": "git",
    "url": "https://github.com/mohitshivv/ecommerceOne.git"
  },
  "bugs": {
    "url": "https://github.com/mohitshivv/ecommerceOne/issues"
  },
  "homepage": "https://github.com/mohitshivv/ecommerceOne#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "autoprefixer": "^10.4.16",
    "formik": "^2.4.5",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
    "react-shimmer": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
